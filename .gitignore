# Ignore generated images and temporary files
output/*
temp/*
!output/.gitkeep
!temp/.gitkeep

# Ignore downloaded models (they're large)
models/checkpoints/*
models/vae/*
models/loras/*
models/controlnet/*
models/unet/*
models/clip/*
models/clip_vision/*
models/style_models/*
models/embeddings/*
models/diffusers/*
models/gligen/*
